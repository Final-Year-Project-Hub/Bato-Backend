{
  "compilerOptions": {
    "target": "ES2020",
    // "module": "node20",
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "include": [
    "src",
    "generated/prisma"
  ],
  "exclude": [
    "node_modules"
  ]
}